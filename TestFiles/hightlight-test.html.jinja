{% extends "base.html" %}
{% block head %}

  <title>{% block title %}{% endblock %}</title>
  
  {% endblock %}
  {% block content%}
    <div>
      <h1>{{ HeadingVariable }}</h1>
      {% for k,v in dictionary.items() %}
      <p id="{{ idVariable }}">
        {% if this in that %}
          Then this thing
        {% elif those not in there %}
          Then another thing
        {% else %}
          Nothing at all
        {% endif %}
      {% endfor %}
    </div>
    <p>
      {% set one = 1 %}
        {% if one == two %}
          {% break %}
        {% else %}
          dance
        {% endif %}
    </p>
    {% block wrong %}
    {% include "modal.html" %}
    {% if correct == False %}
  {% endblock %}
{% endblock %}
